{
 
  "include": ["src/**/*"],
  "compilerOptions": {
    "allowImportingTsExtensions":false,
    "checkJs": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "strict": false,
    "skipLibCheck": true,
    "declaration": true,
    "sourceMap": false,
    "declarationMap": false,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "noCheck": true
  },

 
}
